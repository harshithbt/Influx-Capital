<?xml version='1.0' encoding='utf-8'?>
<widget id="com.icdb.inflexcap" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>icintdb</name>
    <description>
        An Influx Capital web app
    </description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
<hook type="after_prepare" src="scripts/hooks/jetAfterPrepare.js"/>
<plugin name="cordova-plugin-wkwebview-file-xhr"/>
<plugin name="cordova-plugin-wkwebview-engine"/>
</widget>

